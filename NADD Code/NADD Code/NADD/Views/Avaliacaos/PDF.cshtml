@model IEnumerable<NADD.Models.Avaliacao>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PDF</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">
                @Html.DisplayNameFor(model => model.NomeAvaliador)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.ProfessorId)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.PeriodoAno)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.ValorProvaExp)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.ValorQuestExp)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.RefBibliograficas)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.PQuestMultdisc)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Eqdistvquest)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Ppquestcontext)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Observacao)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.QtyQuestoes)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Media)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.TotValor)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Disciplinas)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.NomeAvaliador)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.ProfessorId)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.PeriodoAno)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.ValorProvaExp)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.ValorQuestExp)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.RefBibliograficas)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.PQuestMultdisc)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.Eqdistvquest)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.Ppquestcontext)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.Observacao)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.QtyQuestoes)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.Media)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.TotValor)
            </td>
            <td scope="row">
                @Html.DisplayFor(modelItem => item.Disciplinas.NomeDisciplina)
            </td>
           
        </tr>
}
    </tbody>
</table>
</body>
</html>
